package Lesson; 
import java.util.*; 
public class MarkupTax {
  
  static Scanner console - new Scanner(System.in); 
    public static void main(String[] args) { 
    
      final double MARKUP = 0.10; 
      final double TAX = .06; 
      double item, tax, markup; 
      
      System.out.println("The tax of the item is: " + tax)
      System.out.println("The markup price is: " + markup)
      System.out.print("the final price of the item is: "); 
      System.out.println(item + tax + markup); 
      
    }
